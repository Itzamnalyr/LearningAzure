@using SamLearnsAzure.Models
@model BrowseViewModel
@{
    ViewData["Title"] = "Browse";
}


<h2>Environment: @Model.Environment</h2>
<h3>Browse</h3>
<div class="row">
    <div class="col-md-12">
        <table class="table table-striped table-bordered">
            <tr>
                <th>Theme</th>
            </tr>
            @foreach (Themes item in Model.Themes)
            {
                <tr>
                    <td>
                        <a href="~/Home/Theme?ThemeId=@item.Id">@item.Name</a>
                    </td>
                </tr>
            }
        </table>
    </div>
</div>
